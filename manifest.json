{
  "manifest_version": 3,
  "name": "Help Me Out",
  "description": "Screen Recorder Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "./logo_1.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "nativeMessaging",
    "tabs",
    "offscreen",
    "tabCapture",
    "desktopCapture"
  ]
}
